{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "rules": {
    "max-len": [
      "error",
      {
        "code": 140
      }
    ],
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": "error",
    "no-console": "error",
    "no-multi-spaces": "error",
    "import/prefer-default-export": "off",
    "func-names": "off",
    "no-param-reassign": "off",
    "no-unused-expressions": "off"
  },
  "parser": "babel-eslint"
}
